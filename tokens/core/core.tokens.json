{
	"jf": {
		"color": {
			"neutral": {
				"light": {
					"$type": "color",
					"$value": {
						"colorSpace": "srgb",
						"components": [
							1,
							1,
							1
						],
						"hex": "#ffffff"
					}
				}
			}
		},
		"typography": {
			"font-family": {
				"figtree": {
					"$type": "font-family",
					"$value": [
						"Figtree",
						"ArVerdanaial",
						"sans-serif"
					]
				},
				"noto-sans": {
					"$type": "font-family",
					"$value": [
						"Noto Sans",
						"Arial",
						"sans-serif"
					]
				}
			},
			"font-size": {
				"2xs": {
					"$type": "dimension",
					"$value": {
						"value": 0.625,
						"unit": "rem"
					}
				},
				"xs": {
					"$type": "dimension",
					"$value": {
						"value": 0.75,
						"unit": "rem"
					}
				},
				"sm": {
					"$type": "dimension",
					"$value": {
						"value": 0.875,
						"unit": "rem"
					}
