					}
				},
				"2xl": {
					"$type": "dimension",
					"$value": {
						"value": 1.5,
						"unit": "rem"
					}
				},
				"3xl": {
					"$type": "dimension",
					"$value": {
						"value": 2,
						"unit": "rem"
					}
				}
			},
			"weight": {
				"100": {
					"$type": "number",
